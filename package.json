{
  "name": "tokodesktop",
  "version": "1.0.0",
  "description": "TokoVoip Desktop Client with Radio Interface for out of Game Users.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "dotenv": "^8.2.0",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1"
  },
  "build": {
    "appId": "Toko-Desktop-Client",
    "win": {
      "target": [
        "nsis"
      ],
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "uninstallDisplayName": "downloadClient",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xBlazeTECH/TokoDesktop.git"
  },
  "keywords": [],
  "author": "xBlazeTECH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xBlazeTECH/TokoDesktop/issues"
  },
  "homepage": "https://github.com/xBlazeTECH/TokoDesktop#readme"
}
